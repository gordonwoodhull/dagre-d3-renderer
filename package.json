{
  "name": "dagre-d3-renderer",
  "version": "0.4.19",
  "description": "A D3-based renderer for Dagre",
  "keywords": [
    "graph",
    "dagre",
    "graphlib",
    "renderer"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "node -r babel-register ./node_modules/.bin/gulp js:test && node -r babel-register ./node_modules/.bin/gulp demo:test",
    "upgrade": "yarn-upgrade-all && yarn remove d3 && yarn add d3@3.5.17",
    "build": "node -r babel-register node_modules/.bin/webpack --progress --colors",
    "build:watch": "yarn build -- --watch"
  },
  "dependencies": {
    "d3": "3.5.17",
    "dagre": "^0.7.4",
    "graphlib": "^2.1.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.13",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.1.0",
    "gulp-replace": "^0.6.1",
    "gulp-shell": "^0.6.3",
    "gulp-watch": "^4.3.11",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^2.0.4",
    "mocha": "^3.5.0",
    "phantomjs-prebuilt": "^2.1.15",
    "semver": "^5.4.1",
    "standard": "^10.0.3",
    "uglify-js": "^3.0.28",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0",
    "yarn-upgrade-all": "^0.1.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylingsoft/dagre-d3-renderer.git"
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "dist/**/*.js",
      "demo/**/*.js"
    ]
  }
}
